!\", book:{}}\n    },\n    mounted(){\n        const id= this.$route.params.bookId\n        axios.get(`/api/books/${id}`)\n        .then(response=>{\n            this.book = response.data\n        })\n        .catch(err=>{\n            this.err_msg = err.err_msg\n        })\n    },\n    methods:{\n        currency(value){\n            let num = new Number(value)\n            const result = new Intl.NumberFormat(\"ko-KR\", {\n                style:\"currency\",\n                currency:\"KRW\"\n            })\n            .format(num);\n            return result;\n        }\n    }\n}\n</script>"]}]}